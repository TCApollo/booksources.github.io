	"ruleFindUrl": "",
	"ruleFindList": "",
	"ruleFindName": "",
	"ruleFindAuthor": "",
	"ruleFindKind": "",
	"ruleFindLastChapter": "",
	"ruleFindIntroduce": "",
	"ruleFindCoverUrl": "",
	"ruleFindNoteUrl": "",
	"ruleSearchUrl": "/modules/article/search.php?searchtype=keywords&searchkey=searchKey&page=searchPage|char=gbk",
	"ruleBookUrlPattern": "https://www.x23us.com/book/\\d+$",
	"ruleSearchList": "//dl[@id=\"content\"]//tr[td]",
	"ruleSearchName": "//td[1]/a//text()",
	"ruleSearchAuthor": "//td[3]//text()",
	"ruleSearchKind": "//td[4]/text()",
	"ruleSearchLastChapter": "//td[2]/a//text()",
	"ruleSearchIntroduce": "//td[5]//text()",
	"ruleSearchCoverUrl": "//td[2]/a/@href<js>String(result).replace(/.*\\/(\\d*)\\/(\\d*)\\/$/,\"/files/article/image/$1/$2/$2s.jpg\")</js>",
	"ruleSearchNoteUrl": "//td[1]/a/@href",
	"ruleBookInfoInit": "",
	"ruleBookName": "<js>result.match(/《([^》<]*)/)[1]</js>",
	"ruleBookAuthor": "<js>result.match(/作者：\\s*([^<]*)/)[1]</js>",
	"ruleBookKind": "<js>result.match(/文章类别[^=]*[^>]*>([^<]*)/)[1]</js>",
	"ruleBookLastChapter": "<js>result.match(/最新章节：[^>]*>([^<]*)/)[1]</js>",
	"ruleIntroduce": "//p[@id=\"sidename\"]/../p[2]//text()",
	"ruleCoverUrl": "<js>result.match(/\\/files[^\"]+/)[0]</js>",
	"ruleChapterUrl": "<js>result.match(/\"read\"[^\"]*\"([^\"]*)/)[1]</js>",
	"ruleChapterUrlNext": "",
	"ruleChapterList": ":<a\\s*href=\"(\\d+.html)[^>]*>([^<]*)",
	"ruleChapterName": "$2",
	"ruleContentUrl": "$1",
	"ruleBookContent": "//*[@id=\"contents\"]<js>String(result).replace(/顶\\s*点\\s*小\\s*说[^<]*/g,'')</js>",
	"ruleContentUrlNext": "",
	"httpUserAgent": "",
	"serialNumber": 0,
	"weight": 0,
	"enable": true
}